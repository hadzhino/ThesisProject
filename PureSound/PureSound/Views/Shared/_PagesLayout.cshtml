<body>
    <form>
        <header>
            <div class="header-area ">
                <div id="sticky-header" class="main-header-area">
                    <div class="container-fluid">
                        <div class="header_bottom_border">
                            <div class="row align-items-center">
                                <div class="col-xl-3 col-lg-2">
                                    <div class="logo">
                                        <a asp-controller="Home" asp-action="Index">
                                            <img src="~/img/logo.png" style="height:40px; width:160px; margin-top: -10px">
                                        </a>
                                    </div>
                                </div>

                                <div class="col-xl-6 col-lg-7">
                                    <div class="main-menu  d-none d-lg-block">
                                        <nav>
                                            <ul id="navigation">
                                                <li><a asp-controller="Blog" asp-action="Index">blog</a></li>
                                                @if (this.User.Identity.IsAuthenticated)
                                                {
                                                    <li>
                                                        <a href="#">pages <i class="ti-angle-down"></i></a>
                                                        <ul class="submenu">
                                                            <li><a asp-controller="Artists" asp-action="Artists">Artists</a></li>
                                                            <li><a asp-controller="Tracks" asp-action="Tracks">Tracks</a></li>
                                                        </ul>
                                                    </li>
                                                }
                                                <li>
                                                    <a href="#"> Profile <i class="ti-angle-down"></i></a>
                                                    <ul class="submenu">
                                                        @if (!User.Identity!.IsAuthenticated)
                                                        {
                                                            <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Login">Login</a></li>
                                                            <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Register">Register</a></li>
                                                        }
                                                        else
                                                        {
                                                            <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="MyProfile">My Profile</a></li>
                                                            <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Logout">Logout</a></li>
                                                        }
                                                    </ul>
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="mobile_menu d-block d-lg-none"></div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </header>
    </form>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer">
        <div class="footer_top">
            <div class="container">
                <div class="row">
                    <div class="col-xl-6 col-md-6">
                        <div class="footer_widget">
                            <h3 class="footer_title">
                                About
                            </h3>
                            <p class="sub_text">PureSound is a school project which targets music listeners and people interested in the sphere.</p>
                        </div>
                    </div>
                    <div class="col-xl-5 col-md-5 offset-xl-1">
                        <div class="footer_widget">
                            <h3 class="footer_title">
                                Contact Me
                            </h3>
                            <ul>
                                <li>
                                    <a href="#">
                                        etaleksander411@gmail.com
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        +359 89 53 40 446
                                    </a>
                                </li>
                            </ul>
                            <div class="socail_links">
                                <ul>
                                    <li>
                                        <a href="https://www.facebook.com/profile.php?id=100007847444057&locale=bg_BG">
                                            <i class=" fa fa-facebook" style="margin-top:10px"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="https://www.instagram.com/hadzhinho/">
                                            <i class="fa fa-instagram" style="margin-top:10px"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="copy-right_text">
            <div class="container">
                <div class="footer_border"></div>
                <div class="row">
                    <div class="col-xl-7 col-md-6">
                        <p class="copy_right">
                            Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved
                        </p>
                    </div>
                    <div class="col-xl-5 col-md-6">
                        <div class="footer_links">
                            <ul>
                                <li><a asp-controller="Home" asp-action="Index">home</a></li>
                                @if (this.User.Identity.IsAuthenticated)
                                {
                                    <li><a asp-controller="Tracks" asp-action="Tracks">tracks</a></li>
                                    <li><a asp-controller="Artists" asp-action="Artists">artists</a></li>
                                }
                                <li><a asp-controller="Blog" asp-action="Index">blog</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>

